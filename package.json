{
  "name": "character-sheet-react",
  "version": "0.1.0",
  "description": "A character sheet web app.",
  "main": "index.js",
  "scripts": {
    "build-css": "./node_modules/postcss-cli/bin/postcss -m --use precss -o build/app.css src/app.css",
    "build": "./node_modules/webpack/bin/webpack.js && cp src/index.html build/index.html && cp config/config.js build/config.js && npm run build-css",
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KnutKnutson/character-sheet-react.git"
  },
  "author": "Mark Knutson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KnutKnutson/character-sheet-react/issues"
  },
  "homepage": "https://github.com/KnutKnutson/character-sheet-react#readme",
  "dependencies": {
    "firebase": "^2.4.2",
    "material-ui": "^0.14.4",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-tap-event-plugin": "^0.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.5.1",
    "postcss-scss": "^0.1.7",
    "precss": "^1.4.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
